<h1>New Tweet</h1>
<%= form_with model: @tweet do |form| %>
  <div>
    <%= form.label :user_id %>
    <%= form.text_field :user_id %>
  </div>
  <div>
    <%= form.label :image %>
    <%= form.text_field :image %>
  </div>
  <div>
    <%= form.label :text %>
    <%= form.text_field :text %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end%>